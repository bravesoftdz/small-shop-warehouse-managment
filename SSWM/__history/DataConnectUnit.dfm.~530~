  object ProductsADOQuery: TADOQuery
    Connection = StorageADOConn
    CursorType = ctStatic
    BeforeEdit = OrderItemsADOQBeforeEdit
    Parameters = <>
    SQL.Strings = (
      'SELECT '
      'Pro.ProductTypeID, '
      'Pro.IsInstrument, '
      'Pro.IsManufactured, '
      'Pro.IsMaterial, '
      'Pro.Name PrTypeName, '
      'Pr.ProductID, '
      'Pr.UnitLength, '
      'Pr.UnitWidth, '
      'Pr.UnitWeigth,  '
      'Pr.Name, '
      'Pr.SALEPRICE, '
      'Pr.INCOME_PRICE, '
      'Pr.Measure,'
      'Pr.VIEWIMAGE, '
      'Pr.UnitName,'
      'Pr.QUANTITYINUNIT, '
      'Pr.QISSQUARE, '
      'Pr.PGROUPNAME,'
      'Pr.DESCRIPTION   '
      'FROM ProductType Pro, Product Pr '
      'WHERE Pr.ProductTypeID = Pro.ProductTypeID and Pr.ISOLD=0 '
      'ORDER BY ADDDATETIME DESC ')
    Left = 48
    Top = 64
